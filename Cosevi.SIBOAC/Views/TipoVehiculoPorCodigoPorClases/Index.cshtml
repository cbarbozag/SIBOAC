@model IEnumerable<Cosevi.SIBOAC.Models.TipoVehiculoPorCodigoPorClase>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CodigoTiposVehiculos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodigoClasePlaca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodigoCodigoPlaca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodigoTipoVeh)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaDeInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaDeFin)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoTiposVehiculos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoClasePlaca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoCodigoPlaca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoTipoVeh)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaDeInicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaDeFin)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { codigoTiposVehiculos = item.CodigoTiposVehiculos, codigoClasePlaca = item.CodigoClasePlaca, codigoCodigoPlaca = item.CodigoCodigoPlaca, codigoTipoVeh = item.CodigoTipoVeh}) |
            @Html.ActionLink("Details", "Details", new { codigoTiposVehiculos = item.CodigoTiposVehiculos, codigoClasePlaca = item.CodigoClasePlaca, codigoCodigoPlaca = item.CodigoCodigoPlaca, codigoTipoVeh = item.CodigoTipoVeh }) |
            @Html.ActionLink("Delete", "Delete", new { codigoTiposVehiculos = item.CodigoTiposVehiculos, codigoClasePlaca = item.CodigoClasePlaca, codigoCodigoPlaca = item.CodigoCodigoPlaca, codigoTipoVeh = item.CodigoTipoVeh })
        </td>
    </tr>
}

</table>
